{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "arrow-body-style": "off",
        "linebreak-style": 0,
        "consistent-return": "off",
        "arrow-parens":"off",
        "no-console":"off",
        "no-param-reassign":"off",
        "no-unused-vars":"warn",
        "camelcase":"warn"
    }
}
